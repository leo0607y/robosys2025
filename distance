import sys
from geopy.geocoders import Nominatim
from geopy.distance import great_circle
def main():
    line = sys.stdin.readline().strip()
    if not line:
        sys.exit(1)

    try:
        cities = [c.strip() for c in line.split('/')]
        #スラッシュで区切られなかった際に終了
        if len(cities) < 2:
           sys.exit(1)
        
        city1, city2 = cities[0], cities[1]

        geolocator = Nominatim(user_agent="my_distance_calculator")
        
        location1 = geolocator.geocode(city1)
        location2 = geolocator.geocode(city2)
        #地名検索失敗時
        if not location1 or not location2:
            sys.exit(1)

        coords1 = (location1.latitude, location1.longitude)
        coords2 = (location2.latitude, location2.longitude)

        # 距離計算
        distance_km = great_circle(coords1, coords2).km
        distance_mile = great_circle(coords1, coords2).miles

        print(f"{distance_km:.2f}")
        print(f"{distance_mile:.2f}")
        
        #ネット環境がないなど予期せぬエラー時の処理
    except Exception:
        sys.exit(1)

if __name__ == "__main__":
    main()
